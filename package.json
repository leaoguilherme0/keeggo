{
  "name": "cypress-cucumber-carrinho",
  "version": "1.0.0",
  "scripts": {
    "cypress:open": "cypress open",
    "clean:reports": "rm -f cypress/reports/*.json",
    "cypress:run": "cypress run --reporter mochawesome",
    "generate:report": "node generate-report.js",
    "test": "npm run clean:reports && npm run cypress:run && npm run generate:report"
  },
  "dependencies": {
    "cypress": "^13.7.0",
    "esbuild": "^0.19.2"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}